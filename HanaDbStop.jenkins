pipeline {
    agent {label 'clab'}};
    stages {
        stage('Git clone') {
            steps {
              sh 'ansible-playbook HanaDbStopV2.yml -e "host=$hostname"'
            }
        }
        stage('Stop the HANA database') {
            steps {
                sh 'ansible-playbook HanaDbStopV2.yml -e "host=$hostname"'
            }
        }
    }
}
